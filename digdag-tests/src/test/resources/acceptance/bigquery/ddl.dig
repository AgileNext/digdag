timezone: UTC

+run:
  bq_ddl>:
  dataset: foo
  create_datasets:
    - foo
    - id: bar
      friendlyName: a bar table
      location: US
      access:
        - domain: example.com
        - role: READER
        - specialGroup: projectOwners
        - userByEmail: foo@example.com
      default_table_expiration: 2d
      labels:
        l1: v1
        l2: v2

  drop_datasets:
    - foo
    - bar
  empty_datasets:
    - foo
    - bar
  create_tables:
    - foo.a
    - bar.b
    - c
  drop_tables:
    - foo.a
    - bar.b
    - c
  empty_tables:
    - foo.a
    - bar.b
    - c

+post:
  sh>: touch ${outfile}
